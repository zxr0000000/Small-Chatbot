{
  "manifest_version": 2,
  "name": "Budget Manager",
  "version": "1.0",
  "description": "This extensoin tracks your overall spendings.",
  "icons": {
    "128": "icon128.png",
    "48": "icon48.png",
    "16": "icon16.png"
  },
  "background": {
    "scripts": ["js/eventPage.js"],
    "persistent": false
  },

  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "html/popup.html"
  },
  "permissions": ["http://api.brainshop.ai/"]
}
